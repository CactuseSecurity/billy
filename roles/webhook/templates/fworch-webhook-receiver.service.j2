[Unit]
Description=FW Orch Webhook receiver for github
After=network.target remote-fs.target nss-lookup.target
[Service]
WorkingDirectory={{ billytest_home }}
ExecStart=/usr/bin/python3 {{ billytest_home }}/billy-webhook-receiver.py
StandardOutput=journal
StandardError=journal
SyslogIdentifier=billy-webhook
User=billytest
Environment=
[Install]
WantedBy=multi-user.target
